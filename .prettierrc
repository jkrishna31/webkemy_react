{
	"plugins": [
		"prettier-plugin-tailwindcss"
	],
	"bracketSpacing": true,
	"printWidth": 100,
	"proseWrap": "preserve",
	"semi": true,
	"singleQuote": true,
	"trailingComma": "all",
	"tabWidth": 2,
	"useTabs": false,
	"arrowParens": "always",
	"requirePragma": true,
	"insertPragma": true,
	"endOfLine": "lf"
}